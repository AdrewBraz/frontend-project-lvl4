{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    ["@babel/plugin-proposal-decorators", { "version": "legacy" }],
    ["@babel/plugin-proposal-class-properties"],
    ["@babel/plugin-transform-class-properties"],
    ["@babel/plugin-transform-runtime",
      {
        "regenerator": true
      }
    ]
  ]
}